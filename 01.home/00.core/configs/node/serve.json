{
  "public": "dist",
  "cleanUrls": true,
  "rewrites": [
    { "source": "app/**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000, immutable"
        }
      ]
    }
  ],
  "directoryListing": false,
  "trailingSlash": false,
  "renderSingle": true,
  "symlinks": true,
  "etag": true
}